{
	"$schema": "https://schemas.elgato.com/streamdeck/plugins/layout.json",
	"id": "tft-comp",
	"items": [
		{
			"key": "title",
			"type": "text",
			"rect": [4, 0, 196, 18],
			"font": { "size": 12, "weight": 700 },
			"alignment": "left"
		},
		{
			"key": "comp_name",
			"type": "text",
			"rect": [4, 18, 196, 18],
			"font": { "size": 11 },
			"alignment": "left"
		},
		{
			"key": "comp_info",
			"type": "text",
			"rect": [4, 36, 196, 18],
			"font": { "size": 10 },
			"alignment": "left",
			"color": "#AAAAAA"
		},
		{
			"key": "champ_list",
			"type": "text",
			"rect": [4, 54, 196, 18],
			"font": { "size": 10, "weight": 600 },
			"alignment": "left",
			"color": "#C89B3C"
		},
		{
			"key": "tier_bar",
			"type": "bar",
			"rect": [4, 80, 192, 8],
			"range": { "min": 0, "max": 100 },
			"value": 0,
			"bar_fill_c": "#FF7F50",
			"bar_bg_c": "#333333"
		}
	]
}
