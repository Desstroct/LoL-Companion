{
	"$schema": "https://schemas.elgato.com/streamdeck/plugins/layout.json",
	"id": "jungle-timer",
	"items": [
		{
			"key": "title",
			"type": "text",
			"rect": [0, 0, 200, 22],
			"font": { "size": 13, "weight": 600 },
			"alignment": "center",
			"color": "#E67E22"
		},
		{
			"key": "timer",
			"type": "text",
			"rect": [0, 22, 200, 28],
			"font": { "size": 18, "weight": 700 },
			"alignment": "center"
		},
		{
			"key": "status",
			"type": "text",
			"rect": [0, 50, 200, 20],
			"font": { "size": 11 },
			"alignment": "center",
			"color": "#AAAAAA"
		},
		{
			"key": "progress",
			"type": "bar",
			"rect": [10, 76, 180, 16],
			"range": { "min": 0, "max": 100 },
			"value": 0,
			"bar_fill_c": "#E67E22",
			"bar_bg_c": "#333333"
		}
	]
}
