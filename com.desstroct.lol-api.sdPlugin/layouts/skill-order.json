{
	"$schema": "https://schemas.elgato.com/streamdeck/plugins/layout.json",
	"id": "skill-order",
	"items": [
		{
			"key": "title",
			"type": "text",
			"rect": [10, 0, 180, 22],
			"font": { "size": 13, "weight": 600 },
			"alignment": "left",
			"color": "#FF6666"
		},
		{
			"key": "skill_order",
			"type": "text",
			"rect": [10, 22, 180, 28],
			"font": { "size": 17, "weight": 700 },
			"alignment": "left"
		},
		{
			"key": "skill_info",
			"type": "text",
			"rect": [10, 50, 180, 20],
			"font": { "size": 11 },
			"alignment": "left",
			"color": "#AAAAAA"
		},
		{
			"key": "wr_bar",
			"type": "bar",
			"rect": [10, 74, 180, 16],
			"range": { "min": 0, "max": 100 },
			"value": 50,
			"bar_fill_c": "#2ECC71",
			"bar_bg_c": "#333333"
		}
	]
}
