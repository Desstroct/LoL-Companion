{
	"$schema": "https://schemas.elgato.com/streamdeck/plugins/layout.json",
	"id": "lobby-scanner",
	"items": [
		{
			"key": "title",
			"type": "text",
			"rect": [0, 0, 200, 22],
			"font": { "size": 13, "weight": 600 },
			"alignment": "center",
			"color": "#3498DB"
		},
		{
			"key": "champion",
			"type": "text",
			"rect": [0, 22, 200, 28],
			"font": { "size": 18, "weight": 700 },
			"alignment": "center"
		},
		{
			"key": "rank",
			"type": "text",
			"rect": [0, 50, 100, 20],
			"font": { "size": 11 },
			"alignment": "center",
			"color": "#AAAAAA"
		},
		{
			"key": "wr_text",
			"type": "text",
			"rect": [100, 50, 100, 20],
			"font": { "size": 11, "weight": 600 },
			"alignment": "center",
			"color": "#AAAAAA"
		},
		{
			"key": "wr_bar",
			"type": "bar",
			"rect": [10, 76, 180, 16],
			"range": { "min": 40, "max": 65 },
			"value": 50,
			"bar_fill_c": "#3498DB",
			"bar_bg_c": "#333333"
		}
	]
}
