{
	"$schema": "https://schemas.elgato.com/streamdeck/plugins/layout.json",
	"id": "jungle-path",
	"items": [
		{
			"key": "champ_icon",
			"type": "pixmap",
			"rect": [4, 10, 72, 72]
		},
		{
			"key": "title",
			"type": "text",
			"rect": [82, 0, 118, 22],
			"font": { "size": 12, "weight": 600 },
			"alignment": "left",
			"color": "#2ECC71"
		},
		{
			"key": "path_name",
			"type": "text",
			"rect": [82, 22, 118, 28],
			"font": { "size": 16, "weight": 700 },
			"alignment": "left"
		},
		{
			"key": "path_detail",
			"type": "text",
			"rect": [82, 50, 118, 20],
			"font": { "size": 11 },
			"alignment": "left",
			"color": "#AAAAAA"
		},
		{
			"key": "step_bar",
			"type": "bar",
			"rect": [82, 74, 108, 16],
			"range": { "min": 0, "max": 100 },
			"value": 0,
			"bar_fill_c": "#2ECC71",
			"bar_bg_c": "#333333"
		}
	]
}
