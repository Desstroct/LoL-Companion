{
	"$schema": "https://schemas.elgato.com/streamdeck/plugins/layout.json",
	"id": "jungle-path",
	"items": [
		{
			"key": "champ_icon",
			"type": "pixmap",
			"rect": [4, 6, 72, 72]
		},
		{
			"key": "title",
			"type": "text",
			"rect": [82, 2, 118, 18],
			"font": { "size": 11 },
			"alignment": "left",
			"color": "#2ECC71"
		},
		{
			"key": "path_name",
			"type": "text",
			"rect": [82, 20, 118, 26],
			"font": { "size": 16, "weight": 700 },
			"alignment": "left"
		},
		{
			"key": "camp_route",
			"type": "text",
			"rect": [82, 46, 118, 24],
			"font": { "size": 10 },
			"alignment": "left",
			"color": "#AAAAAA"
		},
		{
			"key": "step_bar",
			"type": "bar",
			"rect": [82, 74, 108, 16],
			"range": { "min": 0, "max": 100 },
			"value": 0,
			"bar_fill_c": "#2ECC71",
			"bar_bg_c": "#333333"
		}
	]
}
